<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC 
    "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" 
    "http://struts.apache.org/dtds/struts-2.1.dtd"> 
<struts>
    <package name="com.zxt.compplatform.indexgenerate" extends="struts-default" namespace="/indexpage">
    	<action name="indexpage" class="pageGenerateAction">
         	<result name="toAdd">/pages/indexTemplate/addIndexPage.jsp</result>
         	<result name="toconfig">/pages/indexTemplate/pageconfig.jsp</result>
         	<result name="toPageConf">/pages/indexTemplate/updateIndexPage.jsp</result>
         	<result name="indexShowPage">/pages/indexPage/template/template${templateLocation}.jsp</result>
        </action>
    </package>
</struts>